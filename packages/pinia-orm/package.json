{
  "name": "pinia-orm",
  "version": "0.14.0",
  "description": "The Pinia plugin to enable Object-Relational Mapping access to the Pinia Store.",
  "keywords": [
    "vue",
    "pinia",
    "pinia-plugin",
    "pinia-orm",
    "orm"
  ],
  "license": "MIT",
  "author": {
    "name": "Gregor Becker",
    "email": "gregor@codedredd.de"
  },
  "funding": "https://github.com/sponsors/codedredd",
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "browser": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --clean --format cjs,esm,iife",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l pinia-orm -r 1",
    "coverage": "vue-demi-switch 3 && vitest --run --coverage",
    "size": "size-limit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "test:ui": "vue-demi-switch 3 && vitest --ui",
    "test:watch": "vue-demi-switch 3 && vitest --watch",
    "test:2": "vue-demi-switch 2 vue2 && vitest --run",
    "test:3": "vue-demi-switch 3 && vitest --run",
    "test": "pnpm run test:3"
  },
  "peerDependencies": {
    "pinia": "^2.0.16"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "normalizr": "^3.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/prettier": "^2",
    "@vitest/ui": "^0.18.1",
    "@vue/composition-api": "^1.7.0",
    "c8": "^7.11.3",
    "core-js": "^3.23.5",
    "eslint": "^8.20.0",
    "happy-dom": "^6.0.4",
    "pinia": "^2.0.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.8",
    "std-env": "^3.1.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vitest": "^0.18.1",
    "vue": "^3.2.37",
    "vue-demi": "^0.13.5",
    "vue2": "npm:vue@^2.7.3"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 kB"
    }
  ],
  "volta": {
    "node": "16.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeDredd/pinia-orm.git"
  },
  "bugs": {
    "url": "https://github.com/CodeDredd/pinia-orm/issues"
  },
  "homepage": "https://github.com/CodeDredd/pinia-orm#readme",
  "pnpm": {
    "packageExtensions": {
      "@vue/composition-api": {
        "peerDependencies": {
          "vue": "*"
        }
      }
    }
  }
}
