{
  "name": "@pinia-orm/normalizr",
  "version": "1.0.1",
  "description": "Normalizes JSON according to schema for Redux and Flux applications",
  "bugs": {
    "url": "https://github.com/CodeDredd/pinia-orm/issues"
  },
  "homepage": "https://github.com/CodeDredd/pinia-orm",
  "repository": {
    "url": "https://github.com/CodeDredd/pinia-orm.git",
    "type": "git"
  },
  "keywords": [
    "flux",
    "redux",
    "normalize",
    "api",
    "json"
  ],
  "files": [
    "dist/",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l @pinia-orm/normalizr -r 1",
    "size": "size-limit",
    "flow": "flow",
    "flow:ci": "flow check",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix --ext .js"
  },
  "author": "Paul Armstrong",
  "contributors": [
    "Dan Abramov",
    "Gregor Becker"
  ],
  "license": "MIT",
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "eslint": "^8.25.0",
    "immutable": "^4.1.0",
    "size-limit": "^8.1.0",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "2 kB"
    }
  ]
}
