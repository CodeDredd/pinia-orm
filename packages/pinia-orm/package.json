{
  "name": "pinia-orm",
  "version": "1.7.2",
  "description": "The Pinia plugin to enable Object-Relational Mapping access to the Pinia Store.",
  "keywords": [
    "vue",
    "pinia",
    "pinia-plugin",
    "pinia-orm",
    "orm"
  ],
  "license": "MIT",
  "author": {
    "name": "Gregor Becker",
    "email": "gregor@codedredd.de"
  },
  "funding": "https://github.com/sponsors/codedredd",
  "jsdelivr": "dist/index.mjs",
  "unpkg": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "import": "./dist/*.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "import": "./dist/*.mjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l pinia-orm -r 1",
    "coverage": "vue-demi-switch 3 && vitest --run --coverage",
    "size": "size-limit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "test:ui": "vue-demi-switch 3 && vitest --ui --api 9527",
    "test:watch": "vue-demi-switch 3 && vitest --watch",
    "test:2": "vue-demi-switch 2 vue2 && vitest --run",
    "test:3": "vue-demi-switch 3 && vitest --run",
    "test": "pnpm run test:3"
  },
  "peerDependencies": {
    "pinia": "^2.1.6"
  },
  "dependencies": {
    "@pinia-orm/normalizr": ">=1.7.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@pinia/testing": "^0.1.3",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/node": "^20.8.8",
    "@types/prettier": "^2.7.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "@vue/composition-api": "^1.7.2",
    "@vue/test-utils": "^2.4.1",
    "c8": "^8.0.1",
    "core-js": "^3.32.2",
    "eslint": "^8.49.0",
    "happy-dom": "^12.1.6",
    "mkdist": "^1.3.0",
    "nanoid": "4.0.2",
    "pinia": "^2.1.6",
    "prettier": "^3.0.3",
    "size-limit": "^11.0.0",
    "std-env": "^3.4.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "uuid": "^9.0.1",
    "vite": "^4.4.9",
    "vitest": "^0.34.5",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.6",
    "vue2": "npm:vue@^2.7.3"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "13 kB"
    },
    {
      "path": "dist/decorators.mjs",
      "limit": "1 kB"
    },
    {
      "path": "dist/casts.mjs",
      "limit": "1 kB"
    },
    {
      "path": "dist/helpers.mjs",
      "limit": "1 kB"
    }
  ],
  "volta": {
    "node": "20.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeDredd/pinia-orm.git"
  },
  "bugs": {
    "url": "https://github.com/CodeDredd/pinia-orm/issues"
  },
  "homepage": "https://github.com/CodeDredd/pinia-orm#readme"
}
