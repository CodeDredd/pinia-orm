{
  "name": "pinia-orm",
  "version": "1.3.0",
  "description": "The Pinia plugin to enable Object-Relational Mapping access to the Pinia Store.",
  "keywords": [
    "vue",
    "pinia",
    "pinia-plugin",
    "pinia-orm",
    "orm"
  ],
  "license": "MIT",
  "author": {
    "name": "Gregor Becker",
    "email": "gregor@codedredd.de"
  },
  "funding": "https://github.com/sponsors/codedredd",
  "jsdelivr": "dist/index.mjs",
  "unpkg": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./decorators": {
      "types": "./dist/decorators.d.ts",
      "require": "./dist/decorators.js",
      "import": "./dist/decorators.mjs"
    },
    "./casts": {
      "types": "./dist/casts.d.ts",
      "require": "./dist/casts.js",
      "import": "./dist/casts.mjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l pinia-orm -r 1",
    "coverage": "vue-demi-switch 3 && vitest --run --coverage",
    "size": "size-limit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "test:ui": "vue-demi-switch 3 && vitest --ui",
    "test:watch": "vue-demi-switch 3 && vitest --watch",
    "test:2": "vue-demi-switch 2 vue2 && vitest --run",
    "test:3": "vue-demi-switch 3 && vitest --run",
    "test": "pnpm run test:3"
  },
  "peerDependencies": {
    "pinia": "^2.0.27"
  },
  "dependencies": {
    "@pinia-orm/normalizr": ">=1.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@pinia/testing": "^0.0.14",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/prettier": "^2",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-c8": "^0.25.5",
    "@vitest/ui": "^0.25.5",
    "@vue/composition-api": "^1.7.1",
    "@vue/test-utils": "^2.2.6",
    "c8": "^7.12.0",
    "core-js": "^3.26.1",
    "eslint": "^8.29.0",
    "happy-dom": "^8.1.0",
    "mkdist": "^1.0.0",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.27",
    "prettier": "^2.8.1",
    "size-limit": "^8.1.0",
    "std-env": "^3.3.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "uuid": "^9.0.0",
    "vite": "^3.2.5",
    "vitest": "^0.25.5",
    "vue": "^3.2.45",
    "vue-demi": "^0.13.11",
    "vue2": "npm:vue@^2.7.3"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "10 kB"
    },
    {
      "path": "dist/decorators.mjs",
      "limit": "1 kB"
    },
    {
      "path": "dist/casts.mjs",
      "limit": "1 kB"
    },
    {
      "path": "dist/helpers.mjs",
      "limit": "1 kB"
    }
  ],
  "volta": {
    "node": "18.12.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeDredd/pinia-orm.git"
  },
  "bugs": {
    "url": "https://github.com/CodeDredd/pinia-orm/issues"
  },
  "homepage": "https://github.com/CodeDredd/pinia-orm#readme",
  "pnpm": {
    "packageExtensions": {
      "@vue/composition-api": {
        "peerDependencies": {
          "vue": "*"
        }
      }
    }
  }
}
