{
  "name": "@pinia-orm/nuxt",
  "description": "Nuxt module for pinia-orm",
  "version": "2.0.0-alpha.2",
  "license": "MIT",
  "author": {
    "name": "Gregor Becker",
    "email": "gregor@codedredd.de"
  },
  "funding": "https://github.com/sponsors/codedredd",
  "keywords": [
    "vue",
    "pinia",
    "pinia-plugin",
    "pinia-orm",
    "orm",
    "nuxt"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l @pinia-orm/nuxt -r 1",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground"
  },
  "peerDependencies": {
    "@pinia/nuxt": ">=0.10.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.3",
    "pinia-orm": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@pinia/nuxt": "^0.10.1",
    "@types/prettier": "^3.0.0",
    "eslint": "^9.6.0",
    "nuxt": "^3.11.2",
    "pinia": "^3.0.1",
    "prettier": "^3.3.2",
    "std-env": "^3.7.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vue": "^3.5.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeDredd/pinia-orm.git"
  },
  "bugs": {
    "url": "https://github.com/CodeDredd/pinia-orm/issues"
  },
  "homepage": "https://github.com/CodeDredd/pinia-orm/tree/v2/packages/nuxt#readme"
}
