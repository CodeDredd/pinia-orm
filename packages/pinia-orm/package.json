{
  "name": "pinia-orm",
  "version": "1.8.0",
  "description": "The Pinia plugin to enable Object-Relational Mapping access to the Pinia Store.",
  "keywords": [
    "vue",
    "pinia",
    "pinia-plugin",
    "pinia-orm",
    "orm"
  ],
  "license": "MIT",
  "author": {
    "name": "Gregor Becker",
    "email": "gregor@codedredd.de"
  },
  "funding": "https://github.com/sponsors/codedredd",
  "jsdelivr": "dist/index.mjs",
  "unpkg": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "import": "./dist/*.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "import": "./dist/*.mjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l pinia-orm -r 1",
    "coverage": "vue-demi-switch 3 && vitest --run --coverage",
    "size": "size-limit",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "test:ui": "vue-demi-switch 3 && vitest --ui --api 9527",
    "test:watch": "vue-demi-switch 3 && vitest --watch",
    "test:2": "vue-demi-switch 2 vue2 && vitest --run",
    "test:3": "vue-demi-switch 3 && vitest --run",
    "test": "pnpm run test:3"
  },
  "peerDependencies": {
    "pinia": "^2.1.7"
  },
  "dependencies": {
    "@pinia-orm/normalizr": ">=1.7.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.3.10",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@pinia/testing": "^0.1.3",
    "@size-limit/preset-small-lib": "^11.1.2",
    "@types/node": "^20.12.10",
    "@types/prettier": "^3.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@vue/composition-api": "^1.7.2",
    "@vue/test-utils": "^2.4.6",
    "c8": "^9.1.0",
    "core-js": "^3.37.0",
    "eslint": "^9.2.0",
    "happy-dom": "^14.10.1",
    "mkdist": "^1.5.1",
    "nanoid": "4.0.2",
    "pinia": "^2.1.7",
    "prettier": "^3.2.5",
    "size-limit": "^11.1.2",
    "std-env": "^3.7.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "uuid": "^9.0.1",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue": "^3.4.27",
    "vue-demi": "^0.14.7",
    "vue2": "npm:vue@^2.7.3"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "13 kB"
    },
    {
      "path": "dist/decorators.mjs",
      "limit": "1 kB"
    },
    {
      "path": "dist/casts.mjs",
      "limit": "1 kB"
    },
    {
      "path": "dist/helpers.mjs",
      "limit": "1 kB"
    }
  ],
  "volta": {
    "node": "20.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeDredd/pinia-orm.git"
  },
  "bugs": {
    "url": "https://github.com/CodeDredd/pinia-orm/issues"
  },
  "homepage": "https://github.com/CodeDredd/pinia-orm#readme"
}
